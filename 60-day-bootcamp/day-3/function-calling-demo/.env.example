# OpenRouter API Configuration
OLLAMA_API_KEY=your_openrouter_api_key_here
OLLAMA_BASE_URL=http://localhost:11434

# Model Configuration
DEFAULT_MODEL=meta-llama/llama-3.1-8b-instruct:free
# Alternative models: google/gemini-2.0-flash-exp:free, qwen/qwen-2.5-7b-instruct:free

# Tool Execution Settings
TOOL_TIMEOUT_SECONDS=5
MAX_TOOL_RETRIES=3

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/tool_execution.jsonl

# API Settings
API_RATE_LIMIT=100